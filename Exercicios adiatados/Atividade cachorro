let idadecachorro = prompt("Digite a idade do seu cachorro ");
let idadehumana = (idadecachorro) * 7;
if (idadehumana >= 63) {
    alert("Seu cachorro tem " + idadehumana + " anos humanos, ele ja poderia ter se aposentado!");
} else if (idadecachorro < 0) {
    alert("Idade invÃ¡lida");
} else {
    alert("Seu cachorro tem " + idadehumana + " anos humanos, e nao pode se aposentar ainda!");
}